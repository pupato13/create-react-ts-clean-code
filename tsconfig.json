{
    "compilerOptions": {
        "target": "es6",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "esModuleInterop": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "jsx": "react",
        "rootDir": "src",
        "baseUrl": "src",
        "allowJs": true,
        "skipLibCheck": true,
        "allowConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true
    },
    "include": ["src"]
}
